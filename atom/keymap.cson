'atom-workspace':
  'alt--': 'unset!'
  'cmd-e': 'recent-files-fuzzy-finder:toggle-finder'
  'cmd-[': 'last-cursor-position:previous'
  'cmd-]': 'last-cursor-position:next'
  'shift-cmd-o': 'advanced-open-file:toggle'
  'cmd-ยง': 'tree-view:toggle-focus'
  'shift-cmd-i': 'bem-levels:video-touch-phone'
  'cmd-k right': 'window:focus-pane-on-right'
  'cmd-k left': 'window:focus-pane-on-left'
  'cmd-k cmd-right': 'pane:split-right-and-copy-active-item'
  'cmd-k cmd-left': 'pane:split-left-and-copy-active-item'

'atom-text-editor':
  'cmd-backspace': 'editor:delete-line'
  'cmd-e': 'unset!'

'atom-text-editor:not([mini])':
  'ctrl-shift-C': 'unset!'
  'shift-cmd-c': 'editor:copy-path'
  'ctrl-c': 'copy-path:copy-basename'
  'ctrl-shift-c': 'copy-path:copy-project-relative-dirname'
  'ctrl-cmd-shift-c': 'copy-path:copy-project-relative-path'

'.platform-darwin atom-text-editor:not(.mini)':
  'shift-cmd-enter': 'goto-definition:go'

'.platform-darwin atom-text-editor[data-grammar~=js]':
    'cmd-enter': 'atom-ternjs:definition'

'atom-workspace atom-text-editor:not([mini])':
  'cmd-[': 'unset!'
  'cmd-]': 'unset!'

'.platform-darwin atom-text-editor[data-grammar~=ts]':
  'cmd-enter': 'typescript:go-to-declaration'
