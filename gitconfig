[user]
    name = tenorok
    email = mail@tenorok.ru
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red
    new = cyan
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    editor = subl -n -w
[alias]
    cp = cherry-pick
    st = status -s
    cl = clone
    ci = commit
    co = checkout
    br = branch
    dc = diff --cached
    unstage = reset HEAD --
    last = log -1 HEAD
    lg  = log --oneline --graph --decorate --all
